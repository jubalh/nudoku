msgid "Movement\n"
msgstr "Movimiento\n"

#: src/main.c:276
msgid " h - Move left\n"
msgstr " h - Mover a la izq\n"

#: src/main.c:277
msgid " j - Move down\n"
msgstr " j - Mover a abajo\n"

#: src/main.c:278
msgid " k - Move up\n"
msgstr " k - Mover a arriba\n"

#: src/main.c:279
msgid ""
" l - Move right\n"
"\n"
